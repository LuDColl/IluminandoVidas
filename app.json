{
  "expo": {
    "name": "IluminandoVidas",
    "slug": "iluminando-vidas",
    "version": "1.0.0",
    "orientation": "portrait",
    "jsEngine": "hermes",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "../splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF2E5"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "jsEngine": "jsc"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "../adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Permita que o APP acesse sua galeria de fotos."
        }
      ]
    ],
    "sdkVersion": "51.0.0"
  }
}